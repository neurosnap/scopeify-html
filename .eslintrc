{
  "extends": "airbnb-base",
  "env": {
    browser: true,
    node: true
  },
  "rules": {
    "no-use-before-define": 0,
    "prefer-arrow-callback": 0,
    "no-continue": 0,
    "no-param-reassign": 0,
    "no-cond-assign": 0,
    "no-console": 0,
    "object-shorthand": 0,
    "prefer-template": 0,
    "import/no-extraneous-dependencies": 0,
    "strict": 0,
    "no-var": 0,
    "vars-on-top": 0
  }
}
